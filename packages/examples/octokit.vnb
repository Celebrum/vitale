{cells:[{kind:2,language:'typescriptreact',value:'import { getSession, html } from "@githubnext/vitale";\nimport { Octokit } from "@octokit/core";\n\n// log in through VS Code\nconst session = await getSession("github", [], { createIfNone: true });\n\nconst octokit = new Octokit({ auth: session.accessToken });\n\nconst res = await octokit.request("GET /repos/{owner}/{repo}/issues", {\n  owner: "githubnext",\n  repo: "vitale",\n});\n\nhtml(res.data.filter(issue => issue.title.includes(\'dependency\')).map(issue => {\n  return `<div>\n    <a href="${issue.html_url}">${issue.title}</a>\n  </div>`\n}).join(\'\'));\n',metadata:{id:'1ikRIWWkIGHARwEMMUM1H'}}]}