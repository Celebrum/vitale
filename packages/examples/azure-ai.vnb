{cells:[{kind:2,language:'typescriptreact',value:'import ModelClient from "@azure-rest/ai-inference";\nimport { AzureKeyCredential } from "@azure/core-auth";\nimport { createSseStream } from "@azure/core-sse";\nimport { getSession } from "@githubnext/vitale";\nimport streamAzureAIResponse from "./streamAzureAIResponse";\n\nconst session = await getSession("github", [], { createIfNone: true });\n\nasync function generate(prompt: string, options?: { model?: "string" }) {\n  const { model = "gpt-4o" } = options || {};\n\n  const client = new ModelClient(\n    "https://int.api.azureml-test.ms/nexus",\n    new AzureKeyCredential(session.accessToken)\n  );\n\n  const response = await client\n    .path("/chat/completions")\n    .post({\n      body: {\n        messages: [\n          {\n            role: "system",\n            content: "You are a helpful assistant.",\n          },\n          { role: "user", content: prompt },\n        ],\n        model,\n        stream: true,\n      },\n    })\n    .asNodeStream();\n\n  return streamAzureAIResponse(createSseStream(response.body));\n}\n',metadata:{id:'fUdPs5uLJxWHELOq-T1-8'}},{kind:2,language:'typescriptreact',value:'import { markdown } from "@githubnext/vitale";\nimport mapStream from "./mapStream";\n\nasync function ducks(model: string) {\n  return mapStream(await generate("tell me about ducks", { model }), markdown);\n}\n',metadata:{id:'uv1v6IOuMoCpzzZESp_yF'}},{kind:2,language:'typescriptreact',value:'ducks("gpt-4o");',metadata:{id:'QLs1QawAPH-pXx6vSifyf'}},{kind:2,language:'typescriptreact',value:'ducks("mistral-small")',metadata:{id:'9gcxffwQaAAOBr-ETANb2'}},{kind:2,language:'typescriptreact',value:'ducks("mistral-large")',metadata:{id:'CioXm0NfmurwoIVcR1J_f'}}]}